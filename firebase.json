      { "source": "/pretty-0bed919d70/**", "destination": "/pretty-0bed919d70/index.html" },

      { "source": "/vantra-qSKe8jlhMtNvudBqrdmcYljQMM42",    "destination": "/vantra-qSKe8jlhMtNvudBqrdmcYljQMM42/index.html" },
